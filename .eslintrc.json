{
	"ecmaFeatures": {
		"modules": true
	},
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": "standard",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"ignorePatterns": ["*.test.js", "*.cjs"],
	"rules": {
		"semi": "off",
		"quotes": "off",
		"no-tabs": "off",
		"comma-dangle": "off"
	}
}
